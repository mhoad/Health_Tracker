<% if @entry.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-error" style="width: 40%">
      The form contains <%= pluralize(@entry.errors.count, "error") %>.
    </div>
    <ul>
    <% @entry.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>

<%= form_for @entry do |f| %>
  <%= f.label :date %>:
  <%= f.text_field :date %>
  <%= f.label :body_fat %>:
  <%= f.text_field :body_fat %>
  <%= f.label :fitocracy_score %>:
  <%= f.text_field :fitocracy_score %>
  <%= f.label :muscle %>:
  <%= f.text_field :muscle %>
  <%= f.label :water %>:
  <%= f.text_field :water %>
  <%= f.label :weight %>:
  <%= f.text_field :weight %>
  <%= f.submit "Track", class: "btn btn-large btn-primary" %>
<% end %>